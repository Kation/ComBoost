@model EntitySearchItem
<p>
    <a class="alert-link">@Model.Name</a>
    @if (Model.Contains != null)
    {
        <span> Contains <a class="alert-link">“@Model.Contains”</a></span>
    }
    @if (Model.Enum != null)
    {
        <span> Equal <a class="alert-link">“@Model.Enum”</a></span>
    }
    @if (Model.Equal.HasValue)
    {
        <span> Equal <a class="alert-link">“@Model.Equal”</a></span>
    }
    @if (Model.Lessthan.HasValue)
    {
        <span> Lessthan <a class="alert-link">“@Model.Lessthan”</a></span>
    }
    @if (Model.LessthanDate.HasValue)
    {
        <span> Lessthan <a class="alert-link">“@Model.LessthanDate”</a></span>
    }
    @if (Model.Morethan.HasValue)
    {
        <span> Morethan <a class="alert-link">“@Model.Morethan”</a></span>
    }
    @if (Model.MorethanDate.HasValue)
    {
        <span> Morethan <a class="alert-link">“@Model.MorethanDate”</a></span>
    }
</p>